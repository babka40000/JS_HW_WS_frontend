{
  "extends": ["airbnb-base", "plugin:import/errors"],
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-underscore-dangle": "off",
    "max-classes-per-file": "warn",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}